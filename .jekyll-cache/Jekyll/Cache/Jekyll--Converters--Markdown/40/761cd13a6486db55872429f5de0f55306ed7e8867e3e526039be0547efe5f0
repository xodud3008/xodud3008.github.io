    input: process.stdin,
    output: process.stdin
});

let input = [];

rl.on('line', function (line){
    input.push(line);

}).on('close',function (){

    let N = Number(input[0]);
    let A = input[1].split(' ').map(Number);

    // ì˜¤ë¦„ì°¨ìˆœìœ¼ë¡œ ì •ë ¬í•˜ëŠ” í•¨ìˆ˜ : Array.sort((a,b) =&gt; a - b)
    A.sort((a, b) =&gt; a - b);

    M = A[A.length-1];
    let arr = []

    for (let i = 0; i&lt;A.length; i++){
        arr.push(A[i]/M*100)
    }
    console.log(arr)

    // forë¬¸ì„ ì‚¬ìš©í•˜ì—¬ í•© êµ¬í•˜ê¸° =&gt; í‹€ë¦¼
    // let sum = 0.0;
    // for (let i = 0; i&lt;arr.length; i++){
