    // ë°°ì—´ì˜ í•© êµ¬í•˜ëŠ” í•¨ìˆ˜ ì‚¬ìš© =&gt; ë§ìŒ
    let avg = arr.reduce((a, b) =&gt; a + b) / N;
    console.log(avg)

    process.exit();
});
